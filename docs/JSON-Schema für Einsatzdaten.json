{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": 'Einsatz',
	"description": 'Feuerwehreinsatz aus dem Feuerwehr-GIS',
	"type": 'Object',
	"properties": {
		"meta": {
			"type": 'object',
			"description": 'Informationen über den Einsatz',
			"properties": {
				"einsatzstichwort": {
					"description": 'Beschreibung des Einsatzes in wenigen Stichworten',
					"type": 'String'
				},
				"einsatzort": {
					"type": 'String'
				},
				"meldender": {
					"type": 'String'
				},
				"objektNr": {
					"description": 'Id der Einsätze',
					"type": 'String'
				},
				"datumUhrzeitGruppe": {
					"type": 'Date'
				}
			}
		},
		"drawnObjects": {
			"type": 'Object',
			"properties" {
				"fields": {
					"type": "Array",
					"items": {
						"index": {
							"type": "integer"
						},
						"captionTop": {
							"type": "String"
						},
						"captionBottom": {
							"type": "String"
						},
						"targetPosition": {
							"description": 'Koordinaten auf der Karte.',
							"$ref": 'http://json-schema.org/geo'
						},
						"taktischesZeichen": {
							"type": "String"
						},
						"fotoID": {
							"type": "ObjectID"
						}
					}
				}
			}
		},
		"map": {
			"description": "Zustand der Karte",
			"type": "Object",
			"properties": {
				"zoom" {
					"description": "Zoomstufe der Karte",
					"type": "integer"
				},
				"center": {
					"description": 'Koordinaten der Karte.',
					"$ref": 'http://json-schema.org/geo'
				},
				"tileServer": {
					"description": "Quelle des Karte",
					"type": "String"
				}
			}
		},
		"taktZeichen": {
			"description": "Taktische Zeichen in der Kranzdarstellung",
			"type": "Object"
			properties: {
				"kranzposition": {
					"description": "Position des TZ im Kranz",
					"type": "integer",
					"minimum": 1
				},
				"kartenposition": {
					"description": 'Koordinaten des Markers auf der Karte.',
					"$ref": 'http://json-schema.org/geo'
				},
				zeichen": {
					"description": 'Das Zeichen, das an der gegebenen Position gespeichert ist.',
					"$ref": 'taktische_Zeichen_Schema' // Das entwirft Jan
				}
			}
		}
	}
}